{
  "name": "my-react",
  "version": "1.0.0",
  "main": "index.js",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
    "eslint": "eslint ./src/** --ignore-pattern *.scss --ignore-pattern *.ttf --no-error-on-unmatched-pattern --quiet",
    "commitlint": "commitlint --config .commitlintrc.js -e"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint",
      "commit-msg": "yarn commitlint"
    }
  },
  "dependencies": {
    "@arco-design/web-react": "^2.47.0",
    "@babel/core": "7.17.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime-corejs3": "7.17.2",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/marked": "^4.0.8",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-sticky": "^6.0.4",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "ahooks": "^3.7.5",
    "babel-loader": "8.2.3",
    "babel-plugin-import": "1.13.3",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "8.10.0",
    "eslint-config-alloy": "4.5.1",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.8",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "mini-css-extract-plugin": "2.5.3",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "sass": "^1.63.6",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "typescript": "^4.9.5",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.2"
  }
}
